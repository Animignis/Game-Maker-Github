            <string>///http stuff

//check for authenticity token
if (ds_map_find_value(async_load, "id") == ah_authenticity) {
    //Grab the authenticity token
    var json, json_data;
    json = ds_map_find_value(async_load, "result");
    json_data = jso_decode_list(json);
    token = jso_list_get(json_data, 0);
    jso_cleanup_list(json_data);
    
    //Form the request
    var highscore_map, xwfu_req;
    highscore_map = jso_new_map();
    jso_map_add_string(highscore_map, "name", name);
    jso_map_add_integer(highscore_map, "score", _score);
    json_data = jso_new_map();
    jso_map_add_submap(json_data, "highscore", highscore_map);
    jso_map_add_string(json_data, "authenticity_token", token);
    xwfu_req = jso_xwfu_encode_map(json_data);
    jso_cleanup_map(json_data);
    
    //Submit the request
    ah_authenticity = -1;
    ah_submit = http_post_string(server_root +
    "/highscores.json", xwfu_req);
    
    status = 1;
    
    //Set the timeout
    alarm[0] = request_timeout*room_speed;
}
//Got reply from create request --- done
else if (ds_map_find_value(async_load, "id") == ah_submit) {
    //Done
    status = 2;
    //Unset the timeout
    alarm[0] = -1;
}
</string>
